/* =============== */
/* = Info blocks = */
/* =============== */

dl.info{
  background: #f1f1f1;
  margin-bottom: 30px;
}

dl.highlight dt,
dl.info dt{
  font-size: 1.2em;
  line-height: 1.38;
  background: #fff;
  padding: 10px 14px;
}

dl.highlight dd,
dl.info dd{
  padding: 10px 14px;
}

dl.info dd div.text p:last-child,
dl.info dd div.text ul:last-child,
dl.info dd div.text ol:last-child,
dl.highlight dd div.text p:last-child,
dl.highlight dd div.text ul:last-child,
dl.highlight dd div.text ol:last-child{
  margin-bottom: 0;
}


dl.highlight ul, dl.info ul {
	list-style: none;
}
/* Highlight */

dl.highlight{
  margin-bottom: 30px;
}

dl.highlight,
dl.highlight.blue,
dl.highlight.purple,
dl.highlight.green,
dl.highlight.orange,
dl.highlight.red{
  background-image: url(../images/backgrounds/dl-shadow.gif);
  background-position: 100% 100%;
  background-repeat: no-repeat;
/*  margin-right: -11px;
  padding-right: 11px; 
  padding-bottom: 11px; 
  margin-bottom: -1px;*/
}

dl.highlight dd{
  background-color: #F1F1F1;
}

dl.highlight ul.links li{
  border-color: #fff;
}

dl.highlight dd li a:hover{
  background: #f7f7f7;
}

dl.highlight dd.more{
  border-top: 1px solid #ddd;
}

dl.highlight.blue dt,
dl.highlight.purple dt,
dl.highlight.green dt,
dl.highlight.orange dt,
dl.highlight.red dt{
  color: #fff;
  background-color: #009dff;
  text-transform: uppercase;
  background-image: url(../images/backgrounds/dl-shadow-dt.gif);
  background-position: 100% 0;
  background-repeat: no-repeat;
/*  margin-right: -11px;
  padding-right: 11px;  */
}

dl.highlight.blue ul.links li{ border-top: 1px solid #fff; }
dl.highlight.blue dd{ background-color: #99d8ff; }
dl.highlight.blue dd li a:hover{ background: #d6efff; }

dl.highlight.purple ul.links li{ border-top: 1px solid #fff; }
dl.highlight.purple dt{ background-color: #9c1467; }
dl.highlight.purple dd{ background-color: #d7a1c2; }
dl.highlight.purple dd li a:hover{ background: #efd9e7; }

dl.highlight.green ul.links li{ border-top: 1px solid #fff; }
dl.highlight.green dt{ background-color: #53c024; }
dl.highlight.green dd{ background-color: #bae6a7; }
dl.highlight.green dd li a:hover{ background: #e3f5dc;}

dl.highlight.orange ul.links li{ border-top: 1px solid #fff; }
dl.highlight.orange dt{ background-color: #fec711; }
dl.highlight.orange dd{ background-color: #ffe9a0; }
dl.highlight.orange dd li a:hover{ background: #fff6d9; }

dl.highlight.red ul.links li{ border-top: 1px solid #fff; }
dl.highlight.red dt{ background-color: #cf1700; }
dl.highlight.red dd{ background-color: #eca299; }
dl.highlight.red dd li a:hover{ background: #f7dad6; }

