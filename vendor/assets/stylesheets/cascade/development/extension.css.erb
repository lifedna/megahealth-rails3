	background: url(<%= asset_path 'search.png' %>) no-repeat 5px 4px;
	width: 100%;
	height: 28px;
	border: solid 1px #ccc;
	padding-left: 30px;
	background-color: #fff;

	-webkit-border-radius: 10em;
	-moz-border-radius: 10em;
	border-radius: 10em;
	
	-webkit-transition: all .5s;
	-moz-transition: all .5s;
	transition: all .5s;
}

input[type=search]:focus {
	border-color: #51a7e8;
	box-shadow: inset 0 1px 2px rgba(0,0,0,0.075),0 0 5px rgba(81,167,232,0.5);
}

/* placeholder */
input:-moz-placeholder {
	color: #999;
}
input::-webkit-input-placeholder {
	color: #999;
}

.health-side-menu li {
	border-top: 1px solid #fff;
}

.health-side-menu li a {
	padding-left: 20px;
}

.health-side-menu li.active a:before {
	border-color: transparent #3981a7 transparent transparent ;
	border-style: solid;
	border-width: 9px;
	bottom: auto;
	left: auto;
	position: absolute;
	right: 0px;
	text-indent: -800em;
	top: 50%;
	z-index: 1;
	margin-top: -8px;
}

.health-side-menu li.active a:after {
	border-color: transparent #fff transparent transparent ;
	border-style: solid;
	border-width: 8px;
	bottom: auto;
	left: auto;
	position: absolute;
	right: -1px;
	text-indent: -800em;
	top: 50%;
	z-index: 2;
	margin-top: -7px;
}

.health-side-menu li.active a {
	background-color: #3981a7;
	border-color: #3981a7;
	border-style: solid;
	border-width: 1px 1px 1px 1px;
	color: #fff;
}

.health-side-menu li.active a:hover {
	background-color: #3981a7;
	border-color: #3981a7;
	border-style: solid;
	border-width: 1px 1px 1px 1px;
	color: #fff;
}

